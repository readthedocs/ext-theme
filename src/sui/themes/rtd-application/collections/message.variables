@headerFontSize: @relativeMedium;
@boxShadow: none;
@borderRadius: 0px;
@messageTextOpacity: 1;

@notificationConsequences: {
  @error: {
    color: @red;
    inverted: true;
  };
  @warning: {
    color: @lightOrange;
    inverted: true;
  };
  @info: {
    color: @teal;
    inverted: true;
  };
  @note: {
    color: @blue;
  };
  @tip: {
    color: @violet;
  };
};
