{
  "name": "readthedocsext-theme",
  "version": "1.0.0",
  "description": "Read the Docs site theme",
  "main": "src/js/index.js",
  "scripts": {
    "clean-js": "rm -v readthedocsext/theme/static/readthedocsext/theme/js/*.js; exit 0",
    "clean-css": "rm -v readthedocsext/theme/static/readthedocsext/theme/css/*.css; exit 0",
    "clean": "npm run clean-js && npm run clean-css",
    "build": "npm run clean && webpack --mode=production --progress --colors",
    "dev": "webpack-dev-server --mode=development",
    "lint": "prettier -c .",
    "format": "prettier --write ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "create-fomantic-icons": "^1.0.4"
  },
  "devDependencies": {
    "ansi_up": "^4.0.4",
    "babel-loader": "^8.1.0",
    "chart.js": "^2.9.3",
    "clipboard": "^2.0.6",
    "css-loader": "^3.1.0",
    "dayjs": "^1.8.29",
    "file-loader": "^3.0.1",
    "imports-loader": "^0.8.0",
    "jquery": "^3.4.1",
    "knockout": "^3.5.1",
    "lato-font": "^3.0.0",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "matchdep": "~1.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "resolve-url-loader": "^2.2.1",
    "sanitize-html": "^1.23.0",
    "semantic-ui-accordion": "^2.3.1",
    "semantic-ui-api": "^2.3.1",
    "semantic-ui-dimmer": "^2.4.0",
    "semantic-ui-dropdown": "^2.4.1",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-modal": "^2.4.1",
    "semantic-ui-popup": "^2.3.1",
    "semantic-ui-progress": "^2.3.1",
    "semantic-ui-search": "^2.3.3",
    "semantic-ui-tab": "^2.3.1",
    "semantic-ui-transition": "^2.3.1",
    "style-loader": "^0.23.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.1",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-watch-files-plugin": "^1.0.3"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/readthedocs/ext-theme.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/readthedocs/ext-theme/issues"
  },
  "homepage": "https://github.com/readthedocs/ext-theme#readme"
}
